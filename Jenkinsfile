node('node1') {
    stage('CheckoutSCM'){
        sh 'echo "checking out again"'
        sh 'sleep 5'
    }
    stage('Compile'){
        sh 'echo "compiled"'
        sh 'sleep 5'
    }
    stage('Testing'){
        sh 'echo "Tested"'
        sh 'sleep 5'
    }
    stage('Integration Test'){
        sh 'echo "Unit testing done"'
        sh 'sleep 5'
    }
    stage('Deployment'){
        sh 'echo "Deployed successfully"'
        sh 'sleep 5'
    }
}